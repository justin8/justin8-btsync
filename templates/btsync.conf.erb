{
  "device_name": "<%= @hostname %>",
  "listening_port" : <%= @listening_port %>,
  "storage_path" : "<%= @storage_path %>",
  "pid_file" : "/var/run/btsync/btsync.pid",
  "use_upnp" : <%= @use_upnp %>,
  "download_limit" : <%= @download_limit %>,
  "upload_limit" : <%= @upload_limit %>,
  "webui" :
  {
    "listen" : "<%= @webui_listen %>"
<%- if @login -%>
    ,"login" : "<%= @login %>"
<%- end -%>
<%- if @password -%>
    ,"password" : "<%= @password %>"
<%- end -%>
    ,"directory_root" : "<%= @directory_root %>"
  }
}
